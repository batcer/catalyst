{
  "name": "@coyoapp/catalyst",
  "version": "0.0.10",
  "description": "Catalyst Design System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coyoapp/catalyst.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/catalyst/catalyst.esm.js",
  "customElements": "dist/custom-elements/custom-elements.json",
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:ci": "stencil build --ci",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:style": "stylelint **/*.scss"
  },
  "dependencies": {
    "@stencil/core": "^2.7.0",
    "loglevel": "^1.8.0",
    "sanitize.css": "^13.0.0"
  },
  "peerDependencies": {
    "@coyoapp/icons": ">=2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@coyoapp/icons": "^2.0.0",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/react-output-target": "^0.2.1",
    "@stencil/sass": "^1.5.2",
    "@stencil/vue-output-target": "^0.6.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.5.1",
    "puppeteer": "^10.0.0",
    "stylelint": "^14.4.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.5"
  }
