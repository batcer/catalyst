{
  "name": "@coyoapp/catalyst",
  "version": "0.0.2",
  "description": "Catalyst Design System",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/catalyst/catalyst.esm.js",
  "customElements": "dist/custom-elements/custom-elements.json",
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier --write .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cem": "cem analyze --stencil --globs ./src/components/**/*.tsx --exclude **/*.spec.tsx **/*.stories.tsx --outdir ./dist/custom-elements",
    "cem:watch": "cem analyze --stencil --globs ./src/components/**/*.tsx --exclude **/*.spec.tsx **/*.stories.tsx --outdir ./dist/custom-elements --watch"
  },
  "dependencies": {
    "@coyoapp/icons": "^2.0.0",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/core": "^2.7.0",
    "loglevel": "^1.8.0",
    "rxjs": "^7.0.0"
  },
  "peerDependencies": {
    "@coyoapp/icons": "^2.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-backgrounds": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/web-components": "^6.4.9",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^2.0.2",
    "prettier": "^2.5.1",
    "puppeteer": "^10.0.0"
  }
}
